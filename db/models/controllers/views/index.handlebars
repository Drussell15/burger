<div class="container">
    <h1> Bubba Boston's Burger Eater</h1>
    <div class="row">
        <div class="col-md-6">
            <h3> Bubba's burgers currently available</h3>
            <ul>
                {{#each burgers}}
                {{#unless devoured}}

                <li class="BubbaList">
                    {{burger_name}}
                    <button class="devourIt btn btn-warning" data-id="{{id}}">Devour IT</button>
                </li>
                {{/unless}}
                {{/each}}
            </ul>
        </div>